(:initial-context
	(:domain
		(sig x boolean)
		(sig y boolean)
		(sig z boolean)
	)
	(:statement 
		(I1 x)
		(I2 y)
		(I3 z)
	)
	(:temporal
		(planning-interval [0 100])
		(duration I1 [10 10])
		(duration I2 [10 10])
		(duration I3 [10 10])
		(before I1 I2 [1 inf])			
		(after I2 I1 [1 inf])			
		(before I2 I3 [1 inf])			
		(after I3 I2 [1 inf])			
		(before I1 I3 [1 inf])			
		(after I3 I1 [1 inf])			
	)
)

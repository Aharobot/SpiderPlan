FlowModule main:		
	modules = "PrologPreprocessor,Solver";
	rules = { 
		Start => PrologPreprocessor;
		PrologPreprocessor => Solver ;
		Solver => Solver.Consistent => Success  ;
		Solver => Solver.Inconsistent => Fail  ;
	};
	keepTimes=true;
	keepStatistics=true;
	verbose=true;
	verbosity=1;

PrologPreprocessor PrologPreprocessor:
	keepStatistics=true;
	verbose=true;
	verbosity=2;
		
SolverStack Solver:		
	solvers = "Domain,Temporal,Math,Cost,Scheduler,Graph,ICSolver,Planner,Finally";
	timeout = 1;
	keepTimes=true;
	keepStatistics=true;
	verbose=true;
	verbosity=3;

DomainSolver Domain:	
	keepStatistics=true;
	verbose=true;
	verbosity=4;


MathSolver Math:
	verbose=true;
	verbosity=2;

CostSolver Cost:
	verbose=true;
	verbosity=3;

FinallySolver Finally:
	verbose=true;
	verbosity=2;
	

STPSolver Temporal:
	static=false;
	keepTimes=true;
	keepStatistics=true;
	verbose=true;
	verbosity=10;	

SchedulingSolver Scheduler:
	verbose=true;
	verbosity=4;

GraphSolver Graph:
	verbose=true;
	verbosity=4;

InteractionConstraintSolver ICSolver:
	consistencyChecker="ICChecker";
	keepTimes=true;
	keepStatistics=true;
	verbose=true;
	verbosity=4;

SolverStack ICChecker:		
	solvers = "Domain,Cost,Temporal,Graph";
	keepTimes=true;
	keepStatistics=true;
	verbose=true;
	verbosity=3;
	optimization=false;
		
SolverStack ConsistencyChecker:		
	solvers = "Temporal,Scheduler";
	keepTimes=true;
	keepStatistics=true;
	verbose=true;
	verbosity=3;
	
OpenGoalResolverAllFlaws Planner1:
	queueStyle="MultiQueue";
	heuristics="HAddReuse";
	consistencyChecker="ConsistencyChecker";
	keepTimes=true;
	keepStatistics=true;
	verbose=true;	
	verbosity=3;
	
ForwardPlanningModule Planner:
	lookAhead=0;
	queueStyle="MultiQueue";
	heuristics="CausalGraph,FastForward"; 
	consistencyChecker="TemporalStepChecker";
	multiEffectSupport=true;
	yahspLookahead=true;
	//t0=0;
	uniqueInitialState=true;
	nodeEquality="ResultingStateBased";
	keepTimes=false;
	keepStatistics=true;
	verbose=true;//false; 
	verbosity=4;

STPSolver TemporalStepChecker:
	historySize=5;
	keepTimes=true;
	static=true;
	keepStatistics=true;
	verbose=true;
	verbosity=10;

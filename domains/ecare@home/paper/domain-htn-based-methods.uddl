(:operator 
	(infer-sensor ?C - concept ?Cost - cost)
	(:preconditions )
	(:effects
		(?E1 (inferring ?C))
		(?E2 (sensor-state ?Sensor) on)	)
	(:constraints
		(:temporal
			(equals ?THIS ?E1)
			(during ?THIS ?E2 [1 inf] [1 inf])
		)
		(:prolog kb
			(capabilitySensor ?Sensor none ?C ?Cost)
		)
		(:graph
			(edge G ?Sensor ?C input-for)
		)
		(:cost (add link-cost ?Cost))
	)
)

(:operator 
	(infer-sensor-target ?C - concept ?L - location ?O - entity ?Sensor - sensor ?Cost - cost)
	(:preconditions 
		(?P1 (at ?Object) ?L)
		(?P2 (at ?Sensor) ?L)
		(?P3 (targeting ?Sensor) ?Object)
	)
	(:effects
		(?E1 (inferring ?C))
		(?E3 (sensor-state ?Sensor) on)	
	)
	(:constraints
		(:temporal
			(equals ?THIS ?E1)
			(during ?THIS ?E3 [1 inf] [1 inf])

			(during ?THIS ?P1 [1 inf] [1 inf])
			(during ?THIS ?P2 [1 inf] [1 inf])
			(during ?THIS ?P3 [1 inf] [1 inf])
		)
		(:prolog kb
			(capabilitySensor ?Sensor (reqTargeting ?Object) ?C ?Cost)
		)
		(:graph
			(edge G ?Sensor ?C input-for)
		)
		(:cost (add link-cost ?Cost))
	)
)


(:operator ;; infer a concept from two other concepts
	(infer-1 ?C - concept ?Cbase - concept ?Cost - cost)
	(:preconditions	)
	(:effects
		(?E1 (inferring ?C))
	)
	(:constraints
		(:task
			(?P1 (inferring ?Cbase))
		)
		(:temporal
			(equals ?THIS ?E1)
			(during ?THIS ?P1 [1 inf] [1 inf])
		)
		(:prolog kb
			(capability1 ?Cbase ?C ?Cost)
		)
		(:graph
			(edge G ?Cbase ?C input-for)
		)
		(:cost (add link-cost ?Cost))
	)
)


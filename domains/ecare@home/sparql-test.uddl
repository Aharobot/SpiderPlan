(:initial-context

	(:include my-query "./my-query.sparql")

	(:include ontology "./smarthome.owl")

	(:sparql
		(query my-query ?x ?y ?z)

		(prefix smh './smarthome.owl')

		(select ?x ?y ?z 
			(where (list 
				?x (rdf type) (sensor pressure)
				?y (rdf type) (object led)
				?y (smh mountedOn) ?oven
				?oven (rdf type) (object oven)
			 ))
		)

	)
)


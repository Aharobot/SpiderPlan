(:initial-context
	(:task
		(?G1 (infer (activity john)))
		(?G2 (infer (eating john)))
		(?G3 (infer (showering john)))
		(?G4 (infer (cooking john)))
		(?G5 (infer (sleeping john)))
	)
	(:temporal
		(at ?G1 [0 100] [200 inf])
		(at ?G2 [0 600] [700 inf])
		(at ?G3 [0 400] [500 inf])
		(at ?G4 [0 100] [1000 inf])
		(at ?G5 [0 700] [900 inf])
	)
)
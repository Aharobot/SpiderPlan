;; Move single truck from loc1 to loc2

(initial-context
	(domain
		( enum truck ( list truck1 ) )
		( enum location ( list loc1 loc2 ) )
	)
	(statement
		( s1 ( at truck1 ) loc1 )
		( s2 ( free loc2 ) true )
	)
	(goal
		( ?g1 ( at truck1 ) loc2 )
	)
	(temporal
		( release s1 [0 0] )
		( release s2 [0 0] )
		( duration s1 [1 inf] )
		( duration s2 [1 inf] )
	)
)
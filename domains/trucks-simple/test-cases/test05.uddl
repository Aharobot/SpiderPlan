;; Cargo a and b have to switch locations. Both locations are 
;; blocked for intervals s9 and s10. Movement needs to be
;; scheduled around these intervals.

(initial-context
	(domain
		( enum truck ( list truck1 truck2 ) )
		( enum location ( list loc1 loc2 ) )
		( enum cargo ( list a b nothing ) )
	)
	(statement
		( s1 ( at truck1 ) loc1 )
		( s2 ( at truck2 ) loc2 )
		( s3 ( free loc1 ) false )
		( s4 ( free loc2 ) false )
		( s5 ( cargo-at a ) loc1 )
		( s6 ( cargo-at b ) loc2 )
		( s7 ( holds truck1 ) nothing )
		( s8 ( holds truck2 ) nothing )

		( s9 ( free loc1 ) false )
		( s10 ( free loc2 ) false )

	)
	(goal
		( ?g1 ( cargo-at a ) loc2 )
		( ?g2 ( cargo-at b ) loc1 )
	)
	(temporal
		( release s1 [0 0] )
		( release s2 [0 0] )
		( release s3 [0 0] )
		( release s4 [0 0] )
		( release s5 [0 0] )
		( release s6 [0 0] )
		( release s7 [0 0] )
		( release s8 [0 0] )
		( duration s1 [1 inf] )
		( duration s2 [1 inf] )
		( duration s3 [1 inf] )
		( duration s4 [1 inf] )
		( duration s5 [1 inf] )
		( duration s6 [1 inf] )
		( duration s7 [1 inf] )
		( duration s8 [1 inf] )
		( at s9 [5 5] [10 10] )
		( at s10 [5 5] [10 10] )
	)
)
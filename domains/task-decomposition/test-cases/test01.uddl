(:initial-context
	(:domain
		(enum aircraft {a1})
		(enum location {l1 l2})
		(enum person {p1})
	)
	(:statement
		(s1 (at p1) l1)
		(s2 (at a1) l2)
	)
	(:task
		(?T1 (transport-person p1 l1 l2))
	)
	(:temporal
		(release s1 [0 0])
		(release s2 [0 0])
	)

	(:include
		( kb
			<begin-escape-syntax>
				travelTime(flight,l1,l2,180,240).
				travelTime(flight,l2,l1,180,240).
			<end-escape-syntax>
		)
	)
)
	



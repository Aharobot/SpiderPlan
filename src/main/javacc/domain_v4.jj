/* Skipping comments and white spaces */

SKIP:
{
	<";" (~["\n"])* "\n">
}

SKIP:
{
        <" ">
| 		<"\r">
|       <"\t">
|       <"\n">
}

/* MAIN STRUCTURES */

TOKEN :
{
    < INCLUDE : ":include" >
|   < EXECUTION : ":execution" >
|   < TEMPORAL : ":temporal" >
|   < COST : ":cost" >
|   < OPTIMIZATION : ":optimization" >
|   < MATH : ":math" >
|   < SET : ":set" >
|   < GRAPH : ":graph" >
|   < PROLOG : ":prolog" >
|   < SPARQL: ":sparql" >
|   < DOMAIN : ":domain" >
|   < GOAL : ":goal" >
|   < TASK : ":task" >
|   < FINALLY : ":finally" >
|   < MINIZINC : ":minizinc" >
|   < RESOURCE : ":resource" >
|   < CONDITIONAL : ":conditional" >
|   < IC: ":ic" >
|   < CONDITION : ":condition" >
|   < SAMPLING : ":sampling" >
|   < CONFIGURATIONPLANNING : ":configuration-planning" >
|   < ROS : ":ros" >
|   < RESOLVER : ":resolver" >
|   < CAPACITY : ":capacity" >
|   < INITIAL : ":initial-context" >
|   < CUSTOM : ":custom" >
|   < SIMULATE : ":simulate" >
|   < STATEMENT : ":statement" >
| 	< OPERATOR : ":operator" >
| 	< PRECONDITIONS : ":preconditions" > 
| 	< EFFECTS : ":effects" >
| 	< CONSTRAINTS : ":constraints" >
|   < ROSPUBLISH : "publish-to" >
|   < ROSSUBSCRIBE : "subscribe-to" >
|   < ROSREGACTION : "register-action" >
|   < ROSGOAL : "is-goal" >
|   < REUSABLE : "reusable" >
}

/* SEPARATORS */

TOKEN :
{
  < OP: "(" >	
| < CP: ")" >
| < OBRACKET: "[" >
| < CBRACKET: "]" >
| < OBRACE: "{" >
| < CBRACE: "}" >
| < BAR : "|" >
| < COMMA: "," >
| < DOT: "." >
| < COLON: ":">
| < PLUS: "+">
| < MINUS: "-">
| < QMARKS: "\"">
| < QPOINT: "?">
}


/* BUILDING BLOCKS */

TOKEN :
{
    < NON_COMPLEX_TERM : (["a"-"z"]|["A"-"Z"]|["0"-"9"]|["_"]|["/"])(["a"-"z"]|["A"-"Z"]|["0"-"9"]|["_"]|["-"]|["/"])* >
		//< NON_COMPLEX_TERM : (["a"-"z"]|["A"-"Z"]|["0"-"9"]|["_"]|["-"]|["/"]|["+"]|["*"]|["<"]|[">"]|["="])+ >
|   < STRING : <  QMARKS > (~["\""])* < QMARKS > >
|   < UFLOAT :  (["0"-"9"])* < DOT > (["0"-"9"])+ > 
}


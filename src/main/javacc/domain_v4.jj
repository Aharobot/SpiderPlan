{	DomainDefinition()  } 

void DomainDefinition() :
{
}
{
	(
		( LOOKAHEAD(2)
			InitialContext()
			| Operator() 
		)
	)+
}

ArrayList<String> List() :
{
	ArrayList<String> l = new ArrayList<String>();
	String t; 
}
{
	< OBRACE >
		(
		  t = NonComplexTerm()
		  {
				l.add(t);
		  }
		)+		
	< CBRACE >
	{
		return l;
	}
}


Integer Integer() :
{
  	String number;
  	Token sign = null;
  	String signStr = "";
}
{
   [ sign = < PLUS > | sign = < MINUS > ] number = NonComplexTerm()
   {
     	if ( sign != null )
     	{
     	  	signStr = sign.image;
     	}
		return Integer.valueOf(signStr + number);
   } 
}

Double Float() :
{
  	Token number;
  	Token sign = null;
  	String signStr = "";
}
{
   [ sign = < PLUS > | sign = < MINUS > ] number = < UFLOAT >
   {
     	if ( sign != null )
     	{
     	  	signStr = sign.image;
     	}
		return Double.valueOf(signStr + number.image);
   } 
}
